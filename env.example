# Supabase 配置
# 请复制此文件为 .env.local 并填入您的凭据

NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# 可选：Supabase Service Key (服务端使用)
# SUPABASE_SERVICE_KEY=your_supabase_service_key

# ===========================================
# 速创 API 配置 (Sora2 / NanoBanana)
# ===========================================
# 速创 API 端点
SUCHUANG_API_ENDPOINT=https://api.wuyinkeji.com

# 速创 API Key
SUCHUANG_API_KEY=your_suchuang_api_key

# ===========================================
# 豆包 API 配置 (火山引擎 ARK)
# ===========================================
# 豆包 API 端点
DOUBAO_API_ENDPOINT=https://ark.cn-beijing.volces.com/api/v3/chat/completions

# 豆包 API 密钥 (在火山引擎控制台获取)
DOUBAO_API_KEY=your_doubao_api_key

# 豆包推理接入点 ID (必须创建视觉模型的推理接入点)
# 步骤: 火山引擎控制台 -> 在线推理 -> 推理接入点 -> 创建 -> 选择 doubao-seed-1.6-vision
# 格式: ep-20240101000000-xxxxx
DOUBAO_ENDPOINT_ID=your_endpoint_id

# ===========================================
# 旧配置 (保留兼容)
# ===========================================
# Sora API Key (备用，优先使用 SUCHUANG_API_KEY)
SORA_API_KEY=your_sora_api_key
