{
  "manifest_version": 3,
  "name": "Tok Factory 商品提取器",
  "version": "1.0",
  "description": "一键提取商品信息到 Tok Factory",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "提取商品数据",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}



